{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "no-underscore-dangle": 0
  },
  "settings": {
    "import/resolve": {
      moduleDirectory: ["node_modules"]
    }
  }
}
